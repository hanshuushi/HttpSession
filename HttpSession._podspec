s.version = '0.0.1'
s.license = { :type => 'MIT', :file => 'LICENSE' }
s.summary = '基于AFNetwrok和ObjectMapper的Swfit网络会话框架'
s.homepage = 'https://github.com/hanshuushi/HttpSession'
s.author = { 'Han Shuushi' => 'hanshuushi@gmail.com' }
s.requires_arc = true
s.ios.deployment_target = '8.0'
